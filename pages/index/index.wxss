/**index.wxss**/
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.usermotto {
  margin-top: 200px;
}

.top_bg
{
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
  width: 750rpx;
  height: 530rpx;
  text-align: center;
  padding-top: 150rpx;
}
.title
{
  font-size: 30rpx;
  color: #333333;
}
.percentage
{
  font-size: 140rpx;
  color: white;
  margin-top: 50rpx;
}
.percent
{
  font-size: 55rpx;
}
.explain
{
  text-align: center;
  margin-top: 110rpx;
  font-size: 30rpx;
  color: #5c6674;
}
.explain text
{
  color: #f2b713;
}
.submit_button
{
  font-size: 48rpx;
  color: white;
  text-align: center;
  padding-top: 16rpx;
  width: 600rpx;
  height: 90rpx;
  background-color: #f2b713;
  border-radius: 30px;
  margin: 30rpx auto 0;
}
.button_explain
{
  font-size: 24rpx;
  color: #333333;
  text-align: center;
  margin-top: 23rpx;
}
.content_title
{
  font-size: 48rpx;
  color: #333333;
  text-align: center;
  margin-top: 115rpx;
}
.content_image
{
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
  width: 750rpx;
  height: 500rpx;
  margin: 50rpx auto 0;
}
.confirm_introduction
{
  font-size: 48rpx;
  color: #333333;
  text-align: center;
  margin-top: 30rpx;
}
.introduction_content
{
  font-size: 30rpx;
  color: #333333;
  text-align: justify;
  margin: 40rpx auto 0;
  width: 650rpx;
  height: auto;
  display: block;
}
.footer_icon
{
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
  width: 330rpx;
  height: 45rpx;
  margin: 35rpx auto 0;
}
.footer_icon1{
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
  width: 155rpx;
  height: 50rpx;
  margin: 50rpx auto 80rpx;
}
.modal
{
  width: 100%;
  height: 2230rpx;
  background-color: rgba(0, 0, 0, 0.5);
  position: absolute;
  top: 0;
  left: 0;
}
.myModal
{
  width: 580rpx;
  height: 600rpx;
  position: absolute;
  top: 360rpx;
  left: 50rpx;
  background-color: white;
  border-radius: 30rpx;
  padding: 65rpx 0 0 60rpx;
}
.name
{
  font-size: 30rpx;
  line-height: 100%;
  color: #333333;
  letter-spacing: 3rpx;
  width: 260rpx;
  margin-top: 30rpx;
}
.line1
{
  width: 520rpx;
  height: 2rpx;
  background-color: #ddd9d9;
  margin-top: 25rpx;
}
.code
{
  font-size: 30rpx;
  color: #955310;
  width: 175rpx;
  height: 40rpx;
  border-left: 1px solid #333333;
  text-align: right;
  position: absolute;
  left: 400rpx;
  top: 180rpx;
}
.section
{
  font-size: 30rpx;
  color: #333333;
  position: absolute;
  top: 397rpx;
  left: 310rpx;
}
.submit
{
  font-size: 36rpx;
  color: white;
  width: 520rpx;
  height: 85rpx;
  background-color: #955310;
  line-height: 85rpx;
  text-align: center;
  vertical-align: middle;
  border-radius: 50rpx;
  margin-top: 60rpx;
}